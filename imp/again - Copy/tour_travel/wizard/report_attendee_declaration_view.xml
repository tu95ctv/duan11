<odoo>
    <data>
    <record id="tour_travel.attendee_declaration_form_view" model="ir.ui.view">
         <field name="name">attendee.declaration.form</field>
         <field name="model">report.attendee.declaration</field>
         <field name="type">form</field>
         <field name="priority">8</field>
         <field name="arch" type="xml"><form string="Attendee Declaration">
					<group>
						<field name="is_ascii"/>
					</group>
					<footer>
                    <button name="print_report" string="Print" type="object" default_focus="1" class="oe_highlight"/>
                    or 
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
				</form>
         </field>
</record>
    </data>
</odoo>